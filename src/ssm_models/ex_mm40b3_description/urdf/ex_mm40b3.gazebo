<gazebo>
  <plugin name="ex_mm40b3_controller" filename="libgazebo_ros_control.so">
    <robotNamespace>ex_mm40b3_controller</robotNamespace>
    <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
