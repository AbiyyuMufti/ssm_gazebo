<gazebo reference="base_link">
  <mu1>0.2</mu1>
  <mu2>0.2</mu2>
  <selfCollide>true</selfCollide>
  <gravity>true</gravity>
  <visual>
    <material>
      <ambient>0.5 0.5 0.5 1</ambient>
      <diffuse>0.5 0.5 0.5 1</diffuse>
      <specular>0.5 0.5 0.5 1</specular>
      <emissive>0 0 0 1</emissive>
    </material>
    <!-- <plugin name="SimpleThrustVisual" filename="libForceVisual.so">
      <topic_name>thrust_visual</topic_name>
      <color>Gazebo/RedBright</color>
    </plugin> -->
    <plugin name="LiftDragVisual" filename="libForceVisual.so">
      <topic_name>lift_drag_visual</topic_name>
      <color>Gazebo/Blue</color>
    </plugin>
  </visual>
</gazebo>

